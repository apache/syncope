        <artifactId>maven-fluido-skin</artifactId>
        <version>1.1</version>
    </skin>

    <body>
        <links>
            <item name="Apache" href="http://www.apache.org/"/>
            <item name="Apache Incubator" href="http://incubator.apache.org"/>
