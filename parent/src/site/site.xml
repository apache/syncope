        <breadcrumbs>
            <item name="Apache Syncope" href="http://incubator.apache.org/syncope"/>
        </breadcrumbs>
        
        <menu ref="reports" inherit="bottom"/>

        <menu name="ASF">
            <item name="How Apache Works" href="http://www.apache.org/foundation/how-it-works.html"/>
