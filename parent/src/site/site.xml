         name="Apache Syncope">
             
    <bannerRight>
        <src>images/apache-incubator-logo.png</src>
        <href>http://incubator.apache.org/</href>
    </bannerRight>

    <bannerLeft>
        <src>images/apache-syncope-logo-small.jpg</src>
        <href>http://incubator.apache.org/syncope</href>
